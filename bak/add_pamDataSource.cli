/subsystem=datasources/xa-data-source=pamDS:add(jndi-name=java:jboss/datasources/pam_ds, driver-name=postgresql, user-name=postgres, password=dummy)
/subsystem=datasources/xa-data-source=pamDS/xa-datasource-properties=ServerName:add(value=localhost)
